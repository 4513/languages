{
    "name": "mibo/languages",
    "type": "library",
    "description": "PHP Library for ISO 639-2, ISO 639-1 language codes",
    "keywords": [
        "PHP",
        "ISO 639",
        "Languages",
        "Language",
        "Codes",
        "Standard",
        "ISO",
        "639-2",
        "639",
        "Language codes",
        "Lang",
        "Langs",
        "ISO 639-2/B",
        "ISO 639-2/T",
        "ISO 639-1"
    ],
    "authors": [
        {
            "name": "Michal Boris",
            "email": "michal.boris27@gmail.com"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MiBo\\Languages\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MiBo\\Languages\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "phpunit/php-invoker": "^4.0",
        "phpstan/phpstan": "^1.5",
        "phpstan/phpstan-strict-rules": "^1.5",
        "jetbrains/phpstorm-attributes": "^1.0",
        "slevomat/coding-standard": "^8.13",
        "phpstan/extension-installer": "^1.3",
        "php-parallel-lint/php-parallel-lint": "^1.3"
    },
    "config": {
        "platform-check": true,
        "lock": false,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
